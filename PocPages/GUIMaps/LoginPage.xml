<ObjectRepository>
<FeatureSet name='Worlds Largest Professional Network'>

	<Element type='Image' name='LinkedinImage'>
		<xpath>//img[@title = 'LinkedIn']</xpath>
		<PageTitle>Worlds Largest Professional Network</PageTitle>
	</Element>	

	<Element type='Textbox' name='UserNameTxtBox'>
		<id>login-email</id>
		<PageTitle>Worlds Largest Professional Network</PageTitle>
	</Element>
	<Element type='Textbox' name='PasswordTxtBox'>
		<id>login-password</id>
		<PageTitle>Worlds Largest Professional Network</PageTitle>
	</Element>
	<Element type='Button' name='SignOnBut'>
		<xpath>//input[@name='submit']</xpath>
		<PageTitle>Worlds Largest Professional Network</PageTitle>
	</Element>
</FeatureSet>
</ObjectRepository>